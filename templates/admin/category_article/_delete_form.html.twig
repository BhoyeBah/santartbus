<form method="post" action="{{ path('app_category_article_delete', {'id': category_article.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" style="display: inline-block;">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category_article.id) }}">
    <button class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
</form>
